{
  "manifest_version": 3,
  "name": "New Tab",
  "version": "0.1.0",
  "description": "A minimal new tab experience.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.open-meteo.com/*",
    "https://geocoding-api.open-meteo.com/*",
    "https://www.thesportsdb.com/*"
  ],
  "minimum_chrome_version": "88"
}


